{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
   
 "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LARAVEL_TMP_PATH": "/tmp",
    "LARAVEL_LOG_PATH": "/tmp/laravel.log"
  },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public"
}
